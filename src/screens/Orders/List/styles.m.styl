@import '../../../common.styl'

.screen
  height 100%
  padding 2%

.screenWrapper
  width 100%
  height 100%

.table
  background #fff


.head
  background #fff
  padding 20px 0
  border-radius 5px

.row
  display flex
  flex-direction row
  justify-content space-around

.body
  background #f5f5f5
  padding 10px 0
  display grid
  grid-template-rows repeat(4, 1fr)
  grid-template-columns: repeat(5, 1fr)
  grid-row-gap 10px
  grid-column-gap 10px

.pagination
  display flex
  flex-direction row
  justify-content center

